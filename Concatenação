#include <stdlib.h>
#include <stdio.h>

struct reg
{
    int conteudo;
    struct reg *prox;
};

typedef struct reg celula;

int main()
{
    celula c1, c2, c3, c4, c5;
    celula *le;

    le = &c1;

    c1.prox = &c2;
    c2.prox = &c3;
    c3.prox = &c4;
    c4.prox = &c5;
    c5.prox = NULL;

    c1.conteudo = 1;
    c2.conteudo = 2;
    c3.conteudo = 3;
    c4.conteudo = 4;
    c5.conteudo = 5;

    celula s1, s2, s3, s4, s5;
    celula *li;

    li = &s1;

    s1.prox = &s2;
    s2.prox = &s3;
    s3.prox = &s4;
    s4.prox = &s5;
    s5.prox = NULL;

    s1.conteudo = 6;
    s2.conteudo = 7;
    s3.conteudo = 8;
    s4.conteudo = 9;
    s5.conteudo = 10;

    concatena_r(le, li);
    imprime (le);

    return 0;
}

void concatena_r (celula *le, celula *li)
{
    if (le->prox == NULL)
    {
        le->prox = li;
        return;
    }
    le = le->prox;
    concatena_r (le, li);
}

void imprime (celula *le)
{
    if (le == NULL) return;
    printf ("%d\n", le->conteudo);
    imprime (le->prox);

}
    
    
